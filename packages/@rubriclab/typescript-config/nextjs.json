{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"allowJs": true,
		"declaration": false,
		"declarationMap": false,
		"incremental": true,
		"jsx": "preserve",
		"lib": ["dom", "dom.iterable", "esnext"],
		"module": "esnext",
		"noEmit": true,
		"plugins": [{"name": "next"}],
		"resolveJsonModule": true,
		"strict": false,
		"target": "es5"
	},
	"display": "Next.js",
	"exclude": ["node_modules"],
	"extends": "./base.json",
	"include": [".next/types/**/*.ts", "next-env.d.ts"]
}
